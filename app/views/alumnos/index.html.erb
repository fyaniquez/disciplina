<p id="notice"><%= notice %></p>

<h1>Alumnos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rude</th>
      <th>Ci</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnos.each do |alumno| %>
      <tr>
        <td><%= alumno.nombre %></td>
        <td><%= alumno.rude %></td>
        <td><%= alumno.ci %></td>
        <td><%= link_to 'Modificar', edit_alumno_path(alumno) %></td>
        <td><%= link_to 'Borrar', alumno, method: :delete, data: { confirm: 'Â¿Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
