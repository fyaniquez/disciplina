<p id="notice"><%= notice %></p>

<h1>Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Seguimiento</th>
      <th>Archivo</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.fecha %></td>
        <td><%= documento.seguimiento_id %></td>
        <td><%= documento.archivo %></td>
        <td><%= documento.descripcion %></td>
        <td><%= link_to 'Show', documento, :class => "button" %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento), :class => "button" %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' }, :class => "button" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path, :class => "button" %>
